{
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "client/dist" }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/client/dist/index.html" }
    ]
  }
  